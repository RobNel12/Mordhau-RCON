{
  "name": "mordhau-rcon",
  "version": "1.2.1",
  "description": "Discord bot that utilizes RCON for Mordhau server and logs punishments with extra features.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tom-Beijner/Mordhau-RCON.git"
  },
  "author": {
    "name": "Tom Beijner",
    "email": "tom.beijner@gmail.com",
    "url": "https://tombeijner.com"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist",
    "start:dev": "cross-env NODE_ENV=development LOG_LEVEL=debug nodemon --ignore .autoUpdater/ dist",
    "prepublishOnly": "yarn build",
    "build": "tsc --skipLibCheck",
    "build:watch": "tsc --skipLibCheck --watch --preserveWatchOutput",
    "dev": "yarn run build && concurrently \"yarn:build:watch\" \"yarn:start:dev\"",
    "migrate": "node dist/migration/migrate.js"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
