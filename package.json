    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "deep-cleaner": "^1.2.1",
    "download-git-repo": "^3.0.2",
    "eris": "^0.15.1",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "lru-cache": "^6.0.0",
    "mongoose": "^5.11.13",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "playfab-sdk": "^2.65.210315",
    "pluralize": "^8.0.0",
    "remove-markdown": "^0.3.0",
    "retext-english": "^3.0.4",
    "retext-profanities": "^6.1.0",
    "retext-stringify": "^2.0.4",
    "slash-create": "^3.2.3",
    "steamid": "^1.1.3",
    "typed-emitter": "^1.3.1",
    "unified": "^9.2.0",
    "walk": "^2.3.14",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/array.prototype.flatmap": "^1.2.2",
    "@types/cli-progress": "^3.8.0",
    "@types/dashdash": "^1.14.0",
    "@types/fs-extra": "^9.0.11",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/pluralize": "^0.0.29",
    "@types/steamid": "^1.1.0",
    "@types/walk": "^2.3.0",
    "@types/winston": "^2.4.4",
    "concurrently": "^5.3.0",
